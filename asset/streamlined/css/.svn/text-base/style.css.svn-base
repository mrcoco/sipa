    background-image:-o-linear-gradient(top,rgba(247,253,255,0.988281) 0%,rgba(195,219,230,0.988281) 100%);
    background-image:-ms-linear-gradient(top,rgba(247,253,255,0.988281) 0%,rgba(195,219,230,0.988281) 100%);

uhtml, body {
    height: 100%;
}

#wrapper {
    min-height: 100%;
    height: auto !important;
    margin: 0 auto -50px;
}

footer, #push {
    height: 50px;
}

body {
    font: normal normal normal 11px/16px 'Lucida Grande', 'Lucida Sans Unicode', Helvetica, Arial, Verdana, sans-serif;
    overflow: 0;
    background: #8ec2da;
}

.no-border {
    border: none !important;
    -moz-border-radius: 0;
    -webkit-border-radius: 0;
    -khtml-border-radius: 0;
    border-radius: 0;
}

a {
    color: rgba(0, 82, 133, 1);
    outline: none;
    text-decoration: none;
}

a:hover {
    text-decoration: underline;
}

abbr, acronym {
    font-size: 85%;
    letter-spacing: .1em;
    text-transform: uppercase;
}

abbr[title], dfn[title] {
    border-bottom: 1px dotted #000;
    cursor: help;
}

abbr[title], acronym[title], dfn[title] {
    border-bottom: 1px dotted #CCC;
    cursor: help;
}

code, kbd, samp, pre, tt, var, .code {
    font-family: Monaco, 'Panic Sans', 'Lucida Console', 'Courier New', Courier, monospace, sans-serif;
}

h1 {
    font-size: 20px;
}

h2 {
    font-size: 18px;
}

h3 {
    font-size: 14px;
}

h4 {
    font-size: 13px;
}

h6 {
    font-size: 11px;
}

h1, h2, h3, h4, h5, h6, p {
    margin: 1em 0;
}

h1, h2, h3 {
    color: #000;
}

h4, h5, h6 {
    color: #444;
    font-weight: 400;
}

h6, p {
    color: #676767;
}

ul, ol {
    padding-left: 2em;
}

li ul, li ol, ul ul, ol ol, dl dd {
    margin-bottom: 0;
    margin-left: 0;
    margin-top: 0;
}

small, sup, sub {
    font-size: 80%;
}

.code {
    background-color: #EAEFF4;
    color: #069;
    overflow: auto;
    padding: 2px 6px;
}

.al {
    text-align: left !important;
}

.ar {
    text-align: right !important;
}

.ac {
    text-align: center !important;
}

.fl {
    float: left !important;
}

.fr {
    float: right !important;
}

body.login {
    height: 0;
    overflow: hidden;
}

.login-box {
    position: absolute !important;
    top: 50%;
    left: 50%;
    width: 350px;
    margin: 0 auto !important;
}

.login-box form {
    border-bottom: 1px solid #DDD;
    margin: 10px 0;
}

.login-box.main-content {
    min-height: 0;
}

#promo {
    border-bottom: 1px solid #000;
    background: 0;
    -pie-background: linear-gradient(top, #f9d835, #f3961c);
    padding: 10px 0;
}

#promo p {
    color: #fff;
    line-height: 26px;
    text-shadow: 0 1px 0 #000;
    margin: 0;
}

#promo .close {
    color: #e83;
    font-family: 'Lucida Grande', 'Lucida Sans Unicode', Helvetica, Arial, Verdana, sans-serif;
    font-size: 10px;
    font-weight: 700;
    text-shadow: 0 1px 1px #fff;
    text-decoration: none;
    padding: 1px 5px;
}

#wrapper>header {
    /*    background: rgba(38,38,38,1) url(../pelangi.png) bottom repeat-x;*/
    background: rgba(38,38,38,1) url(../images/header-backround.png) repeat-x;
    margin-bottom: 20px;
    position: absolute;
    top: 0;
    width: 100%;
    z-index: 999;
    zoom: 1px;
    -webkit-box-shadow:rgba(0, 0, 0, 0.699219) 0px 0px 5px 1px;
    -moz-box-shadow:rgba(0, 0, 0, 0.699219) 0px 0px 5px 1px;
    box-shadow:rgba(0, 0, 0, 0.699219) 0px 0px 5px 1px;
}

#wrapper>header h1 {
    color: #fff;
    font-family: "Lucida Grande", "Lucida Sans Unicode", "Helvetica Neue", Helvetica, Arial, Verdana, sans-serif;
    font-size: 16px;
    line-height: 40px;
    margin-top: 0;
    margin-bottom: 0;
    text-shadow: 0 1px 1px #000;
}

#wrapper>header h1 a {
    color: #fff;
    text-decoration: none;
}

#wrapper>header nav {
    /*    background: transparent url(../images/line_vert.png) repeat-y top left;*/
}

#wrapper>header nav>ul {
    padding-left: 10px;
}

#wrapper>header nav>ul>li {
    display: block;
    float: left;
    position: relative;
    padding: 0 1px 0 5px;
}

#wrapper>header nav>ul>li a {
    color: #eee;
    display: block;
    font-size: 13px;
    line-height: 37px;
    text-decoration: none;
    text-shadow: 0 1px 1px rgba(0, 0, 0, 0.5);
    padding: 0 5px 0 10px;
}

#wrapper>header nav>ul li:hover>a {
    color: #fff;
    text-shadow:rgba(255, 255, 255, 0.808594) 0px 0px 5px;
}

#wrapper>header nav>ul>li:hover:not(.action)>a {
    background: 0;
    -pie-background: linear-gradient(top, #555, #222);
}

#wrapper>header nav>ul>li.active {
    color: #fff;
    font-weight: 700px;
    background-image:-webkit-gradient(linear,0% 0%,0% 100%,color-stop(0.01,rgba(255,231,112,0.984375)),color-stop(0.15,rgba(255,192,46,0.05)),to(rgba(0,0,0,0)));
    background-image:-moz-linear-gradient(top,rgba(255,231,112,0.984375) 1%,rgba(255,192,46,0.05) 15%,rgba(0,0,0,0) 100%);
    background-image:-o-linear-gradient(top,rgba(255,231,112,0.984375) 1%,rgba(255,192,46,0.05) 15%,rgba(0,0,0,0) 100%);
    background-image:-ms-linear-gradient(top,rgba(255,231,112,0.984375) 1%,rgba(255,192,46,0.05) 15%,rgba(0,0,0,0) 100%);
}

#wrapper>header nav>ul>li.action {
    border: 1px solid rgba(255, 255, 255, 0.07);
    border-bottom: 1px solid rgba(255, 255, 255, 0.15);
    margin-top: 6px;
    -moz-border-radius: 4px;
    -webkit-border-radius: 4px;
    -khtml-border-radius: 4px;
    border-radius: 4px;
    margin-right: 4px;
}

#wrapper>header nav>ul>li.action>a.button {
    color: #fff;
    font-size: 10px;
    font-weight: 700;
    line-height: 16px;
    border: 1px solid #111;
    padding: 4px 10px;
}

#wrapper>header nav>ul>li>a span.arrow-down {
    background: transparent url(../images/arrows.png) no-repeat 0 -103px;
    display: inline-block;
    height: 6px;
    margin-left: 8px;
    width: 10px;
}

#wrapper>header nav>ul>li:hover>a span.arrow-down {
    background: transparent url(../images/arrows.png) no-repeat -10px -103px;
}

#wrapper>header nav>ul ul {
    background: #191919;
    -moz-border-radius: 0 0 5px 5px;
    -webkit-border-bottom-left-radius: 5px;
    -webkit-border-bottom-right-radius: 5px;
    -khtml-border-radius: 0 0 5px 5px;
    border-radius: 0 0 5px 5px;
    display: none;
    padding-bottom: 5px;
    position: absolute;
    width: 150px;
    z-index: 999;
    -moz-box-shadow: 0 2px 2px #777;
    -webkit-box-shadow: 0 2px 2px #777;
    -khtml-box-shadow: 0 2px 2px #777;
    box-shadow: 0 2px 2px #777;
}

#wrapper>header nav>ul ul li a {
    line-height: 30px;
    padding: 0 10px;
}

#wrapper>header nav>ul ul li a:hover {
    background: #434343;
}

#wrapper>section {
    padding-top: 60px;
}

.has-promo #wrapper>section {
    padding-top: 110px;
}

.main-section {
    margin-bottom: 20px;
    -moz-border-radius: 5px;
    -webkit-border-radius: 5px;
    -khtml-border-radius: 5px;
    border-radius: 5px;
}

.main-section .content {
    padding: 20px 20px 20px 0;
}

.main-content {
    background: #fff;
    -moz-box-shadow: 0 0 3px rgba(0, 0, 0, 0.5);
    -webkit-box-shadow: 0 0 3px rgba(0, 0, 0, 0.5);
    -khtml-box-shadow: 0 0 3px rgba(0, 0, 0, 0.5);
    box-shadow: 0 0 3px rgba(0, 0, 0, 0.5);
    -moz-border-radius: 5px;
    -webkit-border-radius: 5px;
    -khtml-border-radius: 5px;
    border-radius: 5px;
    min-height: 420px;
    position: relative;
    z-index: 1;
    margin-bottom: 10px;
}
.sidebar-content {
    background: #fff;
    -moz-box-shadow: 0 0 3px rgba(0, 0, 0, 0.5);
    -webkit-box-shadow: 0 0 3px rgba(0, 0, 0, 0.5);
    -khtml-box-shadow: 0 0 3px rgba(0, 0, 0, 0.5);
    box-shadow: 0 0 3px rgba(0, 0, 0, 0.5);
    -moz-border-radius: 2px;
    -webkit-border-radius: 2px;
    -khtml-border-radius: 2px;
    border-radius: 2px;
    min-height: 100px;
    position: relative;
    z-index: 1;
    margin-bottom: 10px;
}

.main-content>header, .sidebar-content>header  {
    background: #eee;
    background-image:-webkit-gradient(linear,0% 0%,0% 100%,from(rgba(247,253,255,0.99)),to(rgba(195,219,230,0.99)));
    background-image:-moz-linear-gradient(top,rgba(247,253,255,0.99) 0%,rgba(195,219,230,0.99) 100%);
    background-image:-o-linear-gradient(top,rgba(247,253,255,0.99) 0%,rgba(195,219,230,0.99) 100%);
    background-image:-ms-linear-gradient(top,rgba(247,253,255,0.99) 0%,rgba(195,219,230,0.99) 100%);
    border-bottom: 1px solid #999;
    -moz-border-radius: 2px;
    -webkit-border-radius: 2px;
    -khtml-border-radius: 2px;
    border-radius: 2px;
    -moz-box-shadow: 0 1px 1px #bbb;
    -moz-box-shadow: 0 1px 1px rgba(0, 0, 0, 0.2);
    -webkit-box-shadow: 0 1px 1px #bbb;
    -webkit-box-shadow: 0 1px 1px rgba(0, 0, 0, 0.2);
    -khtml-box-shadow: 0 1px 1px #bbb;
    -khtml-box-shadow: 0 1px 1px rgba(0, 0, 0, 0.2);
    -pie-box-shadow: none;
    box-shadow: 0 1px 1px rgba(0, 0, 0, 0.2);
}

.main-content>header.right, .sidebar-content>header.right {
    background: #eee;
    background-image:-webkit-gradient(linear,0% 0%,0% 100%,from(rgba(195,219,230,0.988281)),color-stop(0.93,rgba(247,253,255,0.988281)));
    background-image:-moz-linear-gradient(top,rgba(195,219,230,0.988281) 0%,rgba(247,253,255,0.988281) 93%);
    background-image:-o-linear-gradient(top,rgba(195,219,230,0.988281) 0%,rgba(247,253,255,0.988281) 93%);
    background-image:-ms-linear-gradient(top,rgba(195,219,230,0.988281) 0%,rgba(247,253,255,0.988281) 93%);
    border-bottom-width:0px;
    -webkit-box-shadow:rgba(0, 0, 0, 0.199219) 0px -1px 1px 0px;
    -moz-box-shadow:rgba(0, 0, 0, 0.199219) 0px -1px 1px 0px;
    box-shadow:rgba(0, 0, 0, 0.199219) 0px -1px 1px 0px;
    -moz-border-radius: 2px;
    -webkit-border-radius: 2px;
    -khtml-border-radius: 2px;
    border-radius: 2px;
}

.main-content>header h2 {
    font-family: Georgia, 'Times New Roman', 'Bitstream Charter', Times, serif;
    font-weight: 400;
    font-size: 18px;
    text-shadow: 0 1px 0 #fff;
    margin: 0;
    display: inline-block;
    padding: 15px 30px;
}
.main-content>header .header-button {
    float: right;
    padding: 6px 10px 0 0;
    display: inline-block;
    position: relative;
    text-decoration: none;
}
.sidebar-content>header h2 {
    font-family: Georgia, 'Times New Roman', 'Bitstream Charter', Times, serif;
    font-weight: 400;
    font-size: 18px;
    text-shadow: 0 1px 0 #fff;
    margin: 0;
    display: inline-block;

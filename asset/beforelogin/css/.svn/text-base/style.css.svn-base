root {
    display: block;
}

* {
    margin: 0px;
    padding: 0px;
}

body {
    background: #8ec2da;
    /*background-color: #ffc16b;*/
}

.hb {
    border-bottom:2px solid #000000;
    background-color:rgba(255, 119, 0, 0.988281);
    height: 150px;
}

.header {
    background-color: #000000;
    height: 37px;
    padding-top: 5px;
}

.footer {
    background-color: #000000;
    height: 50px;
    padding-top: 5px;
    color: white;
    font-size: 14px;
    text-align: center;
    margin-top: 30px;
}

#right {
    float: right;
}

/*login ~ erase*/
#search input#username, input#ypassword {
    /*float:right;*/
    width: 130px;
    margin-right: 5px;
    color: black;
    border: none;
    padding-left: 8px;
    background: -webkit-gradient(linear, 0% 0%, 0% 100%, from(#FFFFFF), to(#E6E6E6));
    background: -moz-linear-gradient(19% 75% 90deg, #FFFFFF, #E6E6E6);
}

#search fieldset {
    border: none;
}

/*new login*/
#login input#username, input#ypassword {
    width: 200px;
    margin-right: 5px;
    color: black;
    border: none;
    padding-left: 8px;
    background: -webkit-gradient(linear, 0% 0%, 0% 100%, from(#FFFFFF), to(#E6E6E6));
    background: -moz-linear-gradient(19% 75% 90deg, #FFFFFF, #E6E6E6);
}

.submits {
    color: #6e6e6e;
    font: bold 12px Helvetica, Arial, sans-serif;
    text-decoration: none;
    padding: 4px 12px;
    position: relative;
    display: inline-block;
    text-shadow: 0 1px 0 #fff;
    -webkit-transition: border-color .218s;
    -moz-transition: border .218s;
    -o-transition: border-color .218s;
    transition: border-color .218s;
    background: #f3f3f3;
    background: -webkit-gradient(linear, 0% 40%, 0% 70%, from(#F5F5F5), to(#F1F1F1));
    background: -moz-linear-gradient(linear, 0% 40%, 0% 70%, from(#F5F5F5), to(#F1F1F1));
    border: solid 1px #dcdcdc;
    border-radius: 2px;
    -webkit-border-radius: 2px;
    -moz-border-radius: 2px;
}

.submits:hover {
    color: #333;
    border-color: #999;
    text-decoration: none;
    cursor: pointer;
    -moz-box-shadow: 0 2px 0 rgba(0, 0, 0, 0.2);
    -webkit-box-shadow: 0 2px 5px rgba(0, 0, 0, 0.2);
    box-shadow: 0 1px 2px rgba(0, 0, 0, 0.15);
}

h5 a {
    /*float: right;*/
    color: whitesmoke;
    /*margin-top: 6px;
    padding-left: 100px;*/
    text-decoration: none;
}

/*menu*/
.menu {
    width: 800px;
    height: 52px;
    position: relative;
    top: 20px;
    font-family: "Trebuchet MS", sans-serif;
    font-size: 16px;
    font-style: normal;
    font-weight: bold;
    text-transform: uppercase;
}

.item {
    position: relative;
    background-color: #f0f0f0;
    float: right;
    width: 52px;
    margin: 0px 5px;
    height: 52px;
    border: 2px solid #ddd;
    -moz-border-radius: 30px;
    -webkit-border-radius: 30px;
    border-radius: 30px;
    -moz-box-shadow: 1px 1px 3px #555;
    -webkit-box-shadow: 1px 1px 3px #555;
    box-shadow: 1px 1px 3px #555;
    cursor: pointer;
    overflow: hidden;
}

.link {
    left: 2px;
    top: 2px;
    position: absolute;
    width: 48px;
    height: 48px;
}

.icon_faq {
    background: transparent url(../img/icon/faq.png) no-repeat top left;
}

.icon_privacy {
    background: transparent url(../img/icon/privacy.png) no-repeat top left;
}

.icon_terms {
    background: transparent url(../img/icon/terms.png) no-repeat top left;
}

.icon_help {
    background: transparent url(../img/icon/help.png) no-repeat top left;
}

.icon_about {
    background: transparent url(../img/icon/about.png) no-repeat top left;
}

.item_content {
    position: absolute;
    height: 52px;
    width: 220px;
    overflow: hidden;
    left: 56px;
    top: 7px;
    background: transparent;
    display: none;
}

.item_content h2 {
    color: #aaa;
    text-shadow: 1px 1px 1px #fff;
    background-color: transparent;
    font-size: 14px;
}

.item_content a {
    background-color: transparent;
    float: left;
    margin-right: 7px;
    margin-top: 3px;
    color: #bbb;
    text-shadow: 1px 1px 1px #fff;
    text-decoration: none;
    font-size: 12px;
}

.item_content a:hover {
    color: orangered;
}

.item_content p {
    background-color: transparent;
    display: none;
    font-family: "Trebuchet MS";
    font-size: 11px;
}

/*end menu*/

/*jargon ?*/
.jargon {
    background: url(../img/logow.png) no-repeat;
    width: 100%;
    height: 350px;
}

/*line 2*/
.line {
    background: url(../img/hb1.png) repeat-x;
    width: 100%;
    height: 73.082px;
    margin-top: 40px;
    text-align: center;
    font-size: 18px;
    padding-top: 20px;
}

/*form register*/
#registration {
    top: 20px;
    float: right;
    border: none;
    color: #FF9933;
    background: -webkit-gradient(linear, left bottom, left top, color-stop(0, rgb(60, 60, 60)), color-stop(0.74, rgb(43, 43, 43)), color-stop(1, rgb(60, 60, 60)));
    background-color: #331400;
    -moz-border-radius: 10px;
    -webkit-border-radius: 10px;
    border-radius: 10px;
    margin: 10px;
    width: 400px;
}

#registration a {
    color: #CC7A29;
    text-shadow: 0px -1px 0px #000;
    text-decoration: none;
}

#registration h2 {
    color: #FF9933;
    border-bottom: solid #CC7A29 1px;
    -moz-box-shadow: 0px 1px 0px #3a3a3a;
    text-align: center;
    padding: 15px;
    margin: 0px;
    font-size: 24px;
    font-family: "Trebuchet MS", sans-serif;
}

#registration fieldset {
    padding: 10px 10px 20px 10px;
    border: none;
}

input.text {
    /*-webkit-border-radius: 15px;*/
    /*-moz-border-radius: 15px;*/
    /*border-radius: 15px;*/
    border: none;
    font-size: 14px;
    width: 88%;
    margin-bottom: 15px;
    padding: 7px 8px 7px 30px;
    -moz-box-shadow: 0px 1px 0px #777;
    -webkit-box-shadow: 0px 1px 0px #777;
    background: #ddd url('../img/ic.png') no-repeat 4px 5px;
    background: url('../img/ic.png') no-repeat 4px 5px, -moz-linear-gradient(center bottom, rgb(225, 225, 225) 0%, rgb(215, 215, 215) 54%, rgb(173, 173, 173) 100%);
    background: url('../img/ic.png') no-repeat 4px 5px, -webkit-gradient(linear, left bottom, left top, color-stop(0, rgb(225, 225, 225)), color-stop(0.54, rgb(215, 215, 215)), color-stop(1, rgb(173, 173, 173)));
    color: #333;
    text-shadow: 0px 1px 0px #FFF;
}

input#email {
    background-position: 6px 5px;
    background-position: 6px 5px, 0px 0px;
}

input#password {
    background-position: 4px -20px;
    background-position: 4px -20px, 0px 0px;
}

input#name {
    background-position: 4px -46px;
    background-position: 4px -46px, 0px 0px;
}

#registerNew {
    margin-top: 10px;
    margin-right: 20px;
    padding: 0px 10px 0px 10px;
    height: 30px;
    border: #CC5200 1px solid;
    background-color: #FF6600;
    -moz-border-radius: 20px;
    -webkit-border-radius: 20px;
    cursor: pointer;
    color: white;
    font-size: 15px;
    font-family: "Trebuchet MS", sans-serif;
    float: left;
}

#registerNew:hover {
    background-color: #FF7519;
}

#registerNew:active {
    background-color: #FF9147;
}

#registration p {
    position: relative;
}

fieldset label.infield /*js*/
{
    color: #333;
    border: none;
    text-shadow: 0px 1px 0px #fff;
    position: absolute;
    text-align: left;
    top: 3px !important;
    left: 35px !important;
    line-height: 29px;
}

/*end from register*/

/*      Tombol        */

#follow {
    padding: 5px;
    font-size: 13px;
    font-family: "Trebuchet MS", sans-serif;
    -moz-border-radius: 15px;
    -webkit-border-radius: 15px;
    display: inline;
    border: none;
    color: white;
    cursor: pointer;
    text-align: center;
    position: relative;
    background: -webkit-gradient(linear, 0% 0%, 0% 100%, from(#FF6600), to(#FF9147));
    background: -moz-linear-gradient(19% 75% 90deg, #FF6600, #FF9147);
}

#follow:hover {
    background: -webkit-gradient(linear, 0% 0%, 0% 100%, from(#B24700), to(#FF6600));
    background: -moz-linear-gradient(19% 75% 90deg, #B24700, #FF6600);
}

#unfollow {
    padding: 5px;
    font-size: 13px;
    font-family: "Trebuchet MS", sans-serif;
    -moz-border-radius: 15px;
    -webkit-border-radius: 15px;
    display: inline;
    border: none;
    color: white;
    cursor: pointer;
    text-align: center;
    position: relative;
    background: -webkit-gradient(linear, 0% 0%, 0% 100%, from(#FF1919), to(#FF5E5E));
    background: -moz-linear-gradient(19% 75% 90deg, #FF1919, #FF5E5E);
}

#unfollow:hover {
    background: -webkit-gradient(linear, 0% 0%, 0% 100%, from(#B24242), to(#FF1919));
    background: -moz-linear-gradient(19% 75% 90deg, #B24242, #FF1919);
}
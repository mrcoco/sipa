body{
    padding:0;
    margin:0;
}
div#wrap {
    width: 100%;
    height: 100%;
    margin: 0;
    padding: 0;
}
div#header{
    width: 100%;
    height: 45px;
    position: fixed;
/*    z-index: 0;*/
    background: rgba(38,38,38,1) url(../../images/pelangi.png) bottom repeat-x;
}
div#content {
    top: 5px;
    bottom: 0;
    left: 0;
    right: 0;
    margin: 0;
    overflow: hidden;
}
div#viewer {
    margin-top: 45px;
    background:#CCC;
    display: block;
    position: absolute;
    left: 0;
    right: 460px;
    top: 0;
    bottom: 0;
    z-index: 0;
}
div#sidebarviewer {
    background: transparent url(../streamlined/images/line_vert.png) repeat-y top left;        
    position: absolute;
    right: 0;
    width: 460px;
    height: 100%;
    overflow: auto;
/*    z-index: 0;*/
}
#fplogobttm{visibility: hidden;}
.fplogo{visibility: hidden;}
.viewer{margin-top: 0px;padding-top: 0px;}